{
  "version": 2,
  "buildCommand": "npm run build -- --configuration production",
  "outputDirectory": "dist/inn-chats",
  "framework": "angular",
  "routes": [
    {
      "src": "/(robots\\.txt|sitemap\\.xml)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
